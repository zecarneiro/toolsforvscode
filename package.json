{
	"author": {
		"name": "Jos√© C. Noronha"
	},
	"publisher": "jnoronha",
	"name": "toolsforvscode",
	"displayName": "Tools for VSCode",
	"description": "Collapse, Expand, Reload and Enable, Disable, Install and Uninstall Extensions",
	"version": "3.3.0",
	"engines": {
		"vscode": "^1.63.1"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [],
		"configuration": {
			"title": "Tools for VSCode",
			"properties": {
				"toolsforvscode.profiles": {
					"title": "Profiles",
					"type": "array",
					"default": [],
					"description": "Format: [{name: 'exemple', hide: true, data: ['ext1', ...]}, ...] / 'hide' key is optional"
				},
				"toolsforvscode.sqlite-command": {
					"title": "Sqlite Command",
					"type": "string",
					"default": null,
					"description": "Sqlite command or executable file"
				}
			}
		},
		"viewsContainers": {
			"activitybar": [
				{
					"id": "tools-vscode-jnoronha",
					"title": "Tools VSCode",
					"icon": "./images/toolsvscode.svg"
				}
			]
		},
		"views": {
			"tools-vscode-jnoronha": [
				{
					"id": "profiles-manager-tools-jnoronha",
					"name": "Profiles Manager"
				},
				{
					"id": "others-tools-jnoronha",
					"name": "Others"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "node ./scripts/build/build.js build development ./src/extension.ts --binName extension.js  -v",
		"package": "node ./scripts/build/build.js build production ./src/extension.ts --binName extension.js  -v",
		"watch": "tsc -watch -p tsconfig.json",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js",
		"clean": "node ./scripts/build/build.js clean"
	},
	"files": [
		"dist/**/*",
		"images/**/*",
		"files/**/*"
	],
	"devDependencies": {
		"@types/fs-extra": "^9.0.13",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/prompt-sync": "^4.1.1",
		"@types/vscode": "^1.63.1",
		"@typescript-eslint/eslint-plugin": "^5.1.0",
		"@typescript-eslint/parser": "^5.1.0",
		"eslint": "^8.0.1",
		"eslint-cli": "^1.1.1",
		"eslint-config-google": "^0.14.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"ts-loader": "^8.0.11",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0",
		"webpack": "^5.10.0",
		"webpack-cli": "^4.2.0",
		"yargs": "^17.2.1"
	},
	"dependencies": {
		"@types/chalk": "^2.2.0",
		"@types/xlsx": "^0.0.36",
		"chalk": "^4.1.2",
		"console-markdown": "^1.0.0",
		"fs-extra": "^10.0.0",
		"moment": "^2.29.1",
		"node-ts-js-utils": "^1.0.1",
		"prompt-sync": "^4.2.0",
		"vscode-ts-js-utils": "^1.0.0",
		"xlsx": "^0.17.3",
		"zx": "^4.2.0"
	},
	"homepage": "https://github.com/zecarneiro/toolsforvscode#readme",
	"repository": {
		"type": "git",
		"url": "https://github.com/zecarneiro/toolsforvscode"
	},
	"license": "MIT"
}
