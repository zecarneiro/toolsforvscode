{
	"author": {
		"name": "Jos√© C. Noronha"
	},
	"publisher": "jnoronha",
	"name": "toolsforvscode",
	"displayName": "Tools for VSCode",
	"description": "Collapse, Expand, Reload and Enable, Disable, Install and Uninstall Extensions",
	"version": "3.1.0",
	"engines": {
		"vscode": "^1.46.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [],
		"configuration": {
			"title": "Tools for VSCode",
			"properties": {
				"toolsforvscode.profiles": {
					"title": "Profiles",
					"type": "array",
					"default": [],
					"description": "Format: [{name: 'exemple', hide: true, data: ['ext1', ...]}, ...] / 'hide' key is optional"
				},
				"toolsforvscode.sqlite-command": {
					"title": "Sqlite Command",
					"type": "string",
					"default": "",
					"description": "Sqlite command or executable file"
				}
			}
		},
		"viewsContainers": {
			"activitybar": [
				{
					"id": "tools-vscode-jnoronha",
					"title": "Tools VSCode",
					"icon": "./images/toolsvscode.svg"
				}
			]
		},
		"views": {
			"tools-vscode-jnoronha": [
				{
					"id": "profiles-manager-tools-jnoronha",
					"name": "Profiles Manager"
				},
				{
					"id": "vscode-tools-jnoronha",
					"name": "VSCode"
				},
				{
					"id": "others-tools-jnoronha",
					"name": "Others"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "npm run pre:build && npm run lint:fix && tsc -p tsconfig.json",
		"package": "npm run pre:build && webpack --mode production --devtool hidden-source-map --config ./node-extension.webpack.config.js",
		"watch": "tsc -watch -p tsconfig.json",
		"pretest": "npm run compile && npm run lint:fix",
		"test": "node ./out/test/runTest.js",
		"pre:build": "node ./scripts/prepare-to-build.js",
		"lint": "eslint src scripts --ext ts,js",
		"lint:fix": "npm run lint -- --fix"
	},
	"files": [
		"dist/**/*",
		"scripts/**/*",
		"images/**/*",
		"files/**/*"
	],
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.46.0",
		"@typescript-eslint/eslint-plugin": "^5.1.0",
		"@typescript-eslint/parser": "^5.1.0",
		"eslint": "^8.0.1",
		"eslint-cli": "^1.1.1",
		"eslint-config-google": "^0.14.0",
		"fs-extra": "^10.0.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"ts-loader": "^8.0.11",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0",
		"webpack": "^5.10.0",
		"webpack-cli": "^4.2.0"
	},
	"dependencies": {
		"@types/xlsx": "^0.0.36",
		"node-vscode-utils": "^1.0.0",
		"xlsx": "^0.17.3"
	}
}
