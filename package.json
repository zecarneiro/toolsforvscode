{
	"author": {
		"name": "Jos√© C. Noronha"
	},
	"publisher": "jnoronha",
	"name": "toolsforvscode",
	"displayName": "Tools for VSCode",
	"description": "Collapse, Expand, Reload and Enable, Disable, Install and Uninstall Extensions",
	"version": "3.0.2",
	"engines": {
		"vscode": "^1.46.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./dist/src/extension.js",
	"contributes": {
		"commands": [],
		"configuration": {
			"title": "Tools for VSCode",
			"properties": {
				"toolsforvscode.profiles": {
					"title": "Profiles",
					"type": "array",
					"default": [],
					"description": "Format: [{name: 'exemple', hide: true, data: ['ext1', ...]}, ...] / 'hide' key is optional"
				}
			}
		},
		"viewsContainers": {
			"activitybar": [
				{
					"id": "tools-vscode-jnoronha",
					"title": "Tools VSCode",
					"icon": "./images/toolsvscode.svg"
				}
			]
		},
		"views": {
			"tools-vscode-jnoronha": [
				{
					"id": "profiles-manager-tools-jnoronha",
					"name": "Profiles Manager"
				},
				{
					"id": "vscode-tools-jnoronha",
					"name": "VSCode"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "npm run pre:build && tsc -p ./",
		"package": "npm run pre:build && webpack --mode production --devtool hidden-source-map --config ./build/node-extension.webpack.config.js",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js",
		"pre:build": "node ./scripts/prepare-to-build.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"ts-loader": "^8.0.11",
		"typescript": "^3.8.3",
		"utils": "file:sub-projects/utils",
		"vscode-test": "^1.3.0",
		"webpack": "^5.10.0",
		"webpack-cli": "^4.2.0"
	}
}
