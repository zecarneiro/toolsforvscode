{
  "author": {
    "name": "Jos√© C. Noronha"
  },
  "publisher": "jnoronha",
  "name": "toolsforvscode",
  "displayName": "Tools for VSCode",
  "description": "Collapse, Expand, Reload and Open Terminal",
  "version": "4.0.0",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "Terminal",
    "Collapse",
    "Expand",
    "Reload"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": []
  },
  "scripts": {
    "vscode:prepublish": "npm run clean && npm run esbuild-base -- --minify",
    "esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=dist/extension.js --external:vscode --format=cjs --platform=node",
    "esbuild": "npm run esbuild-base -- --sourcemap",
    "esbuild-watch": "npm run esbuild-base -- --sourcemap --watch",
    "compile": "tsc -p ./",
    "clean": "rimraf dist"
  },
  "files": [
    "dist/**/*",
    "files/**/*"
  ],
  "devDependencies": {
    "@types/cli-progress": "^3.11.5",
    "@types/fs-extra": "^11.0.4",
    "@types/marked": "^6.0.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "^18.19.6",
    "@types/prompt-sync": "^4.2.3",
    "@types/vscode": "^1.85.0",
    "@types/yargs": "^17.0.32",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8",
    "copyfiles": "^2.4.1",
    "esbuild": "^0.19.11",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  },
  "homepage": "https://github.com/zecarneiro/toolsforvscode#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/zecarneiro/toolsforvscode"
  },
  "license": "MIT",
  "dependencies": {
    "fs-extra": "^11.2.0",
    "marked": "^11.1.1",
    "marked-terminal": "^6.2.0",
    "moment": "^2.30.1",
    "nodejs-file-downloader": "^4.12.1",
    "prompt-sync": "^4.2.0",
    "yargs": "^17.7.2"
  }
}
