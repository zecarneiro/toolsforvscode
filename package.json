{
	"author": {
		"name": "Jos√© C. Noronha"
	},
	"publisher": "josecnoronha",
	"name": "toolsforvscode",
	"displayName": "Tools for VSCode",
	"description": "Collapse, Expand, Reload and Enable, Disable, Install and Uninstall Extensions",
	"version": "2.0.1",
	"engines": {
		"vscode": "^1.46.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "toolsforvscode.profilesmanager",
				"title": "Extensions Manager Profile: Show Profiles"
			},
			{
				"command": "toolsforvscode.extensionsinstall",
				"title": "Extensions Manager Profile: Install all extensions in profile data settings"
			},
			{
				"command": "toolsforvscode.extensionsuninstall",
				"title": "Extensions Manager Profile: Uninstall all extensions in profile data settings"
			}
		],
		"configuration": {
			"title": "Tools for VSCode",
			"properties": {
				"toolsforvscode.profiles": {
					"title": "Profiles",
					"type": "array",
					"default": [],
					"description": "Format: [{name: 'exemple', data: ['ext1', ...]}, ...]"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./ && npm run compile:setpermission && npm run compile:script",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js",
		"compile:setpermission": "chmod +x scriptcompile.sh",
		"compile:script": "bash -c './scriptcompile.sh'"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.46.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"@types/sqlite3": "^3.1.6",
		"sqlite": "^4.0.17",
		"sqlite3": "^5.0.0"
	}
}
